{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 17.1",
    "not safari <= 17.1"
  ]
}